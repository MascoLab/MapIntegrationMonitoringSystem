<script type="text/javascript">

BIX5.chart.create("chart1", "chartHolder1", "", "100%", "100%");

var layoutStr =
           '<BIX5 backgroundColor="#FFFFFF" borderStyle="none">'
             +'<Options>'
                  +'<Caption text="GDP Growth of the BRICS Countries"/>'
                    +'<SubCaption text="From 2011 To 2013 (Annual %)" textAlign="center" />'
                  +'<Legend defaultMouseOverAction="false" />'
              +'</Options>'
             +'<Column2DChart showDataTips="true" columnWidthRatio="0.55" selectionMode="single">'
                 +'<horizontalAxis>'
                       +'<CategoryAxis categoryField="Month"/>'
                  +'</horizontalAxis>'
                  +'<verticalAxis>'
                     +'<LinearAxis maximum="10"/>'
                 +'</verticalAxis>'
                    +'<series>'
                       +'<Column2DSeries labelPosition="outside" yField="2011" displayName="2011" showValueLabels="[2]">'
                            +'<showDataEffect>'
                               +'<SeriesInterpolate/>'
                           +'</showDataEffect>'
                      +'</Column2DSeries>'
                      +'<Column2DSeries labelPosition="outside" yField="2012" displayName="2012" showValueLabels="[2]">'
                            +'<showDataEffect>'
                               +'<SeriesInterpolate/>'
                           +'</showDataEffect>'
                      +'</Column2DSeries>'
                      +'<Column2DSeries labelPosition="outside" yField="2013" displayName="2013" showValueLabels="[2]">'
                            +'<showDataEffect>'
                               +'<SeriesInterpolate/>'
                           +'</showDataEffect>'
                      +'</Column2DSeries>'
                  +'</series>'
              +'</Column2DChart>'
           +'</BIX5>';
 
var chartData = [{"Month":"Jan", "2011":2.4, "2012":3.1, "2013":2.5},
 {"Month":"Feb", "2011":3.8, "2012":4.6, "2013":1.3},
    {"Month":"Mar", "2011":8.1, "2012":9, "2013":5.0},
  {"Month":"Apr", "2011":5.1, "2012":4, "2013":7.7},
  {"Month":"May", "2011":2.1, "2012":3.9, "2013":1.9},
    {"Month":"Jun", "2011":5.2, "2012":4.9, "2013":1.9},
    {"Month":"Jul", "2011":4.2, "2012":3.2, "2013":1.9}];
 
BIX5.chart.calls("chart1", {
  "setLayout" : layoutStr,
    "setData" : chartData
});
</script>

</head>
<body>

	<div class="gGrid1">
		<!-- grid -->
