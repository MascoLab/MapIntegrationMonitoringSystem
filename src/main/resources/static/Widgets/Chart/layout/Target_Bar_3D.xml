<BIX5 backgroundColor="#FFFFFF"  borderStyle="none">
	<Options>
		<Caption text="목표 대비 실적 차트" fontFamily="맑은 고딕"/>
		<SubCaption text="Cylinder Bar Type"/>
	</Options>
	<NumberFormatter id="numfmt" useThousandsSeparator="true"/> 
	<Combination3DChart showDataTips="true">
		<verticalAxis>
			<CategoryAxis id="vAxis" categoryField="Month"/> 
		</verticalAxis>
		<horizontalAxis>
			<LinearAxis id="hAxis" formatter="{numfmt}" interval="10" maximum="100"/>
		</horizontalAxis>
		<series>
			<HTarget3DResultSeries xField="Result" displayName="Result" barWidthRatio="0.5" labelPosition="outside" showValueLabels="[6,7]" outsideLabelXOffset="6" outsideLabelYOffset="-2">
				<fill>
					<SolidColor color="#43b3e6"/>
				</fill>
				<showDataEffect>
					<SeriesInterpolate/> 
				</showDataEffect>
			</HTarget3DResultSeries>
			<HTarget3DGoalSeries xField="Goal" displayName="Goal" barWidthRatio="0.52">
				<showDataEffect>
					<SeriesInterpolate/> 
				</showDataEffect>
				<fill>
					<SolidColor color="#bde3fa" alpha="0.5"/>
				</fill>
			</HTarget3DGoalSeries>
		</series>
		<verticalAxisRenderers>
			<Axis3DRenderer axis="{vAxis}" showLine="true" tickPlacement="none">
				<axisStroke>
					<Stroke color="#555555"/>
				</axisStroke>
			</Axis3DRenderer>
		</verticalAxisRenderers>
		<horizontalAxisRenderers>
			<Axis3DRenderer axis="{hAxis}"/>
		</horizontalAxisRenderers>
		<backgroundElements>
			<GridLines/>
		</backgroundElements>
	</Combination3DChart>
</BIX5>