<BIX5 backgroundColor="#FFFFFF" borderStyle="none">
	<Options>
		<Caption text="Bubble Transparency" fontFamily="맑은 고딕" fontSize="12" color="#777777"/>
		<Legend defaultMouseOverAction="true"/>
	</Options>
	<NumberFormatter id="numFmt" precision="0" useThousandsSeparator="true"/>
	<Bubble2DChart showDataTips="true" minRadius="5" maxRadius="40">
		<verticalAxis>
			<LinearAxis maximum="3200"/>
		</verticalAxis>
		<series>
			<Bubble2DSeries displayName="A Dept." xField="A_Profit" yField="A_Cost" radiusField="A_Revenue">
				<fill>
					<SolidColor color="#40b1e6" alpha="0.5"/>
				</fill>
				<stroke>
					<Stroke color="#40b1e6"/>
				</stroke>
				<showDataEffect>
					<SeriesInterpolate/> 
				</showDataEffect>
			</Bubble2DSeries>
			<Bubble2DSeries displayName="B Dept." xField="B_Profit" yField="B_Cost" radiusField="B_Revenue">
				<fill>
					<SolidColor color="#21cbc2" alpha="0.5"/>
				</fill>
				<stroke>
					<Stroke color="#21cbc2"/>
				</stroke>
				<showDataEffect>
					<SeriesInterpolate/> 
				</showDataEffect>
			</Bubble2DSeries>
			<Bubble2DSeries displayName="C Dept." xField="C_Profit" yField="C_Cost" radiusField="C_Revenue">
				<fill>
					<SolidColor color="#fdc323" alpha="0.5"/>
				</fill>
				<stroke>
					<Stroke color="#fdc323"/>
				</stroke>
				<showDataEffect>
					<SeriesInterpolate/> 
				</showDataEffect>
			</Bubble2DSeries>
			<Bubble2DSeries displayName="D Dept." xField="D_Profit" yField="D_Cost" radiusField="D_Revenue">
				<fill>
					<SolidColor color="#4d6fd4" alpha="0.5"/>
				</fill>
				<stroke>
					<Stroke color="#4d6fd4"/>
				</stroke>
				<showDataEffect>
					<SeriesInterpolate/> 
				</showDataEffect>
			</Bubble2DSeries>
		</series>
	</Bubble2DChart>
</BIX5>
