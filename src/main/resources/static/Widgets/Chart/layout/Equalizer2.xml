<BIX5 backgroundColor="#FFFFFF"  borderStyle="none">
    <Options>
        <Caption text="World Top 10 - Fastest Growing Economies" />
        <SubCaption text="GDP Growth (In %)" textAlign="center" />
    </Options>
    <SeriesInterpolate id="ss"/>
    <Equalizer2DChart showDataTips="true" selectionMode="single" columnWidthRatio="0.65" fillsRange="[0,25,50]" gutterTop="20">
        <horizontalAxis>
            <CategoryAxis categoryField="Country"/>
        </horizontalAxis>
        <verticalAxis>
            <LinearAxis maximum="105" interval="10"/>
        </verticalAxis>
        <series>
            <Equalizer2DSeries labelPosition="outside" yField="GDP" displayName="GDP Growth (In %)" showDataEffect="{ss}">
            	<fills>
                    <SolidColor color="#40b2e6"/>
                    <SolidColor color="#20cbc2"/>
                    <SolidColor color="#f7921c"/>
            	</fills>
            </Equalizer2DSeries>
        </series>
    </Equalizer2DChart>
</BIX5>