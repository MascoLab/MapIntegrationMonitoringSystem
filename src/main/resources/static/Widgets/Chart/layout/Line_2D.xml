<BIX5 backgroundColor="#FFFFFF"  borderThickness="1" borderStyle="none">
    <Options>
        <Caption text="Rainfall" />
        <SubCaption text="( mm )" textAlign="right" />
        <Legend />
    </Options>
    <NumberFormatter id="numFmt" precision="0"/>
    <Line2DChart showDataTips="true" dataTipDisplayMode="axis" paddingTop="0">
        <horizontalAxis>
            <CategoryAxis categoryField="Month"/>
        </horizontalAxis>
        <verticalAxis>
            <LinearAxis minimum="0" maximum="100" interval="10" />
        </verticalAxis>
        <series>
            <Line2DSeries yField="goods" displayName="goods">
                <showDataEffect>
                    <SeriesClip duration="1000"/>
                </showDataEffect>
            </Line2DSeries>
            <Line2DSeries yField="income" displayName="income">
                <showDataEffect>
                    <SeriesClip duration="1000"/>
                </showDataEffect>
            </Line2DSeries>
            <Line2DSeries yField="service" displayName="service">
                <showDataEffect>
                    <SeriesClip duration="1000"/>
                </showDataEffect>
            </Line2DSeries>
        </series>
        <annotationElements>
            <CrossRangeZoomer enableZooming="false" horizontalLabelFormatter="{numFmt}" horizontalStrokeEnable="false"/>
        </annotationElements>
    </Line2DChart>
</BIX5>
