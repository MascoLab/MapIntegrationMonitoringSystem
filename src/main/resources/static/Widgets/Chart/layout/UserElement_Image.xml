<BIX5 backgroundColor="#FFFFFF"  borderStyle="none">
    <Options>
        <Caption text="매출 실적 차트"  color="#555555" fontWeight="bold"/>
    </Options>
    <Column2DChart showDataTips="true" columnWidthRatio="0.46">
        <horizontalAxis>
            <CategoryAxis categoryField="Month" padding="0.5"/>
        </horizontalAxis>
        <verticalAxis>
            <LinearAxis id="vAxis" maximum="100"/>
        </verticalAxis>
        <series>
            <Column2DSeries yField="Data" displayName="Data">
                <fill>
                    <SolidColor color="#5587a2"/>
                </fill>
                <showDataEffect>
                    <SeriesInterpolate/>
                </showDataEffect>
            </Column2DSeries>
        </series>
        <annotationElements>
            <AxisMarker fontSize="15">
                <lines>
                    <AxisLine value="70" label="목표" fontFamily="맑은 고딕">
                        <stroke>
                            <Stroke color="#ef562d" weight="2"/>
                        </stroke>
                    </AxisLine>
                </lines>
            </AxisMarker>
        </annotationElements>
    </Column2DChart>
</BIX5>
